# S3 backend configuration (get from 00-state-bucket outputs after deployment)
AWS_ACCESS_KEY_ID=your-s3-access-key-id
AWS_SECRET_ACCESS_KEY=your-s3-secret-access-key

# STACKIT credentials
STACKIT_PROJECT_ID=your-stackit-project-id
STACKIT_SERVICE_ACCOUNT_KEY_PATH=/path/to/your/sa-key.json

# Harbor robot account credentials (for pulling images)
# Create a robot account in Harbor UI and use those credentials here

TF_VAR_harbor_url=harbor.example.com
TF_VAR_harbor_robot_username='robot$myproject+myrobot'
TF_VAR_harbor_robot_token=changeme-your-token-here
